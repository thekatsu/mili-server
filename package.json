{
  "name": "mili-server",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "inngest:dev": "npx inngest-cli@latest dev --no-discovery -u http://localhost:3000/api/inngest",
    "postinstall": "prisma generate && prisma migrate deploy",
    "lint": "next lint",
    "prettier": "prettier --write --ignore-unknown .",
    "prettier:check": "prettier --check --ignore-unknown .",
    "prisma:baseline": "npx prisma migrate diff --from-empty --to-schema-datamodel src/prisma/schema.prisma --script > src/prisma/migrations/0_init/migration.sql",
    "prisma:dev": "npx prisma migrate dev",
    "prisma:pull": "npx prisma db pull",
    "seed": "node -r dotenv/config ./scripts/seed.js",
    "start": "next start"
  },
